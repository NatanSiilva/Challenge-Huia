{
  "name": "Desafio-Huia",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\"",
    "lint-fix": "npm run lint -- --fix",
    "migrations:gen": "ts-node-dev ./node_modules/.bin/typeorm migration:generate",
    "migrations:run": "ts-node-dev ./node_modules/.bin/typeorm migration:run",
    "migrations:rev": "ts-node-dev ./node_modules/.bin/typeorm migration:revert",
    "migrations:cre": "ts-node-dev ./node_modules/.bin/typeorm migration:create",
    "start": "node src/server.ts",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.37",
    "typeorm-pagination": "^2.0.3",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/morgan": "^1.9.3",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.17.10",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.10",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
